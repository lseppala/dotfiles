{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".git/"],
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
    "usePlaceholders": true,
    "completeUnimported": true
  },
  "coc.preferences.formatOnSaveFiletypes" : ["css", "Markdown", "typescript", "typescriptreact", "javascript", "javascriptreact"]
}
